{
    "name": "YouTube Tools",
    "version": "0.0.0",
    "description": "",
    "manifest_version": 3,
    "author": "",
    "action":{
        "default_popup": "index.html",
        "default_title": "YouTube Tools"
    },
    "permissions": [ 
      "tabs", 
      "activeTab",
      "scripting"
  ],
  "host_permissions": [
    "https://*/*", 
    "http://*/*" 
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon.png"
  }
}
